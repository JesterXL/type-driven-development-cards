{
  "name": "elm-live-tailwind-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "elm-live src/Main.elm --dir=public -- --output=public/elm.js",
    "styles": "tailwindcss -i ./src/styles.css -o ./public/tailwind.css --watch",
    "styles:build": "tailwindcss -i ./src/styles.css -o ./public/tailwind.css --minify"
  },
  "devDependencies": {
    "elm": "^0.19.1-6",
    "elm-live": "^4.0.2",
    "tailwindcss": "^3.4.17"
  }
}
